<?xml version="1.0"?>

<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
         xmlns:ins="http://www.mozilla.org/inspector#">

  <rdf:Seq about="inspector:viewers">
    <rdf:li><rdf:Description 
      ins:uid="dom"
      ins:description="DOM Viewer"
      ins:icon="chrome://inspector/content/viewers/dom/dom-icon.gif">
        <ins:filter><![CDATA[
          return doesQI(object, 'nsIDOMDocument');
        ]]></ins:filter>
     </rdf:Description></rdf:li>

    <rdf:li><rdf:Description 
      ins:uid="stylesheets"
      ins:description="Stylesheets">
        <ins:filter><![CDATA[
          return doesQI(object, 'nsIDOMDocument');
        ]]></ins:filter>
     </rdf:Description></rdf:li>

    <rdf:li><rdf:Description 
      ins:uid="domNode"
      ins:description="DOM Node">
        <ins:filter><![CDATA[
          return doesQI(object, 'nsIDOMNode') && object.nodeType != 9;
        ]]></ins:filter>
     </rdf:Description></rdf:li>

    <rdf:li><rdf:Description 
      ins:uid="boxModel"
      ins:description="Box Model">
        <ins:filter><![CDATA[
          return doesQI(object, 'nsIDOMElement');
        ]]></ins:filter>
     </rdf:Description></rdf:li>

    <rdf:li><rdf:Description 
      ins:uid="xblBindings"
      ins:description="XBL Bindings">
        <ins:filter><![CDATA[
          return doesQI(object, 'nsIDOMElement');
        ]]></ins:filter>
     </rdf:Description></rdf:li>

    <rdf:li><rdf:Description
      ins:uid="styleRules"
      ins:description="CSS Style Rules">
        <ins:filter><![CDATA[
          return doesQI(object, 'nsIDOMElement');
        ]]></ins:filter>
     </rdf:Description></rdf:li>

    <rdf:li><rdf:Description 
      ins:uid="computedStyle"
      ins:description="Computed Style">
        <ins:filter><![CDATA[
          return doesQI(object, 'nsIDOMElement');
        ]]></ins:filter>
     </rdf:Description></rdf:li>

    <rdf:li><rdf:Description 
      ins:uid="jsObject"
      ins:description="Javascript Object">
        <ins:filter><![CDATA[
          return true;
        ]]></ins:filter>
     </rdf:Description></rdf:li>

  </rdf:Seq>

</rdf:RDF>
